{
  "name": "unstructured-ds",
  "version": "0.1.1",
  "description": "A comprehensive React design system with 1000+ icons, components, and design tokens",
  "author": {
    "name": "Amruthesh",
    "email": "amrutheshamru16@gmail.com",
    "url": "https://amruthesh.vercel.app"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "design-system",
    "icons",
    "ui-components",
    "vuesax",
    "typescript",
    "design-tokens"
  ],
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs",
      "types": "./dist/components/index.d.ts"
    },
    "./icons": {
      "import": "./dist/icons/index.js",
      "require": "./dist/icons/index.cjs",
      "types": "./dist/icons/index.d.ts"
    },
    "./icons/*": {
      "import": "./dist/icons/*/index.js",
      "require": "./dist/icons/*/index.cjs",
      "types": "./dist/icons/*/index.d.ts"
    },
    "./styles": "./dist/styles/index.css",
    "./styles/*": "./dist/styles/*.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && npm run build:tokens && npm run build:icons && npm run build:lib",
    "build:tokens": "node style-dictionary.config.js",
    "build:icons": "node scripts/generate-icons.cjs",
    "build:lib": "vite build && tsc -p tsconfig.lib.json --noEmitOnError false || echo \"TypeScript warnings present but build continues\"",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "tokens:watch": "nodemon --watch src/tokens --ext json --exec npm run tokens",
    "dev:full": "npm run tokens && npm-run-all --parallel dev tokens:watch"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^5.0.5",
    "style-dictionary": "^4.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.3.0"
  },
  "sideEffects": [
    "*.css"
  ]
}
